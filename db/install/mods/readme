Any Mod linkings you wish to be installed when the block is installed should be placed here.

This should be in a JSON file called "mods.json"

Example:

[
	{
		"mod" : "assign",
		"modtable" : "assign",
		"modcoursecol" : "course",
		"modstartcol" : "allowsubmissionsfromdate",
		"modduecol" : "duedate",
		"modtitlecol" : "name",
		"submissiontable" : "assign_submission",
		"submissionusercol" : "userid",
		"submissiondatecol" : "timecreated",
		"submissionmodcol" : "assignment",
		"auto" : 1,
		"enabled" : 1,
		"deleted" : 0,
		"submissionstatuscol" : "status",
		"submissionstatusval" : "submitted",
		"parttable" : null,
		"partmodcol" : null,
		"parttitlecol" : null,
		"submissionpartcol" : null
	},
        
        {
		"mod" : "turnitintool",
		"modtable" : "turnitintool",
		"modcoursecol" : "course",
		"modstartcol" : "dtstart",
		"modduecol" : "dtdue",
		"modtitlecol" : "name",
		"submissiontable" : "turnitintool_submissions",
		"submissionusercol" : "userid",
		"submissiondatecol" : "submission_modified",
		"submissionmodcol" : "turnitintoolid",
		"auto" : 1,
		"enabled" : 1,
		"deleted" : 0,
		"submissionstatuscol" : "submission_status",
		"submissionstatusval" : "",
		"parttable" : "turnitintool_parts",
		"partmodcol" : "turnitintoolid",
		"parttitlecol" : "partname",
		"submissionpartcol" : "submission_part"
	},
        
	{
		"mod" : "turnitintooltwo",
		"modtable" : "turnitintooltwo",
		"modcoursecol" : "course",
		"modstartcol" : "dtstart",
		"modduecol" : "dtdue",
		"modtitlecol" : "name",
		"submissiontable" : "turnitintooltwo_submissions",
		"submissionusercol" : "userid",
		"submissiondatecol" : "submission_modified",
		"submissionmodcol" : "turnitintooltwoid",
		"auto" : 1,
		"enabled" : 1,
		"deleted" : 0,
		"submissionstatuscol" : "submission_status",
		"submissionstatusval" : "",
		"parttable" : "turnitintooltwo_parts",
		"partmodcol" : "turnitintooltwoid",
		"parttitlecol" : "partname",
		"submissionpartcol" : "submission_part"
	}
]
